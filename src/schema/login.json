  "$schema": "http://json-schema.org/draft-04/schema#",
  
  "type": "object",

  "title": "Login User schema",

  "required": [
    "email",
    "password"
  ],

  "properties": {
    "email": {
      "type": "string",
      "format": "email"
    },
    "password": {
      "type": "string"
    }
  }
}