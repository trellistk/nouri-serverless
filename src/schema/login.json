{
  "$schema": "http://json-schema.org/draft-04/schema#",
  
  "type": "object",

  "title": "Login User schema",

  "required": [
    "email",
    "password",
    "chapter"
  ],

  "properties": {
    "chapter": {
      "type": "string",
      "enum": ["seattle-eastside"]
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "password": {
      "type": "string"
    }
  }
}